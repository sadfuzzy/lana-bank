{
  "$defs": {
    "AccountStatus": {
      "enum": [
        "inactive",
        "active"
      ],
      "type": "string"
    },
    "AuditEntryId": {
      "format": "int64",
      "type": "integer"
    },
    "AuditInfo": {
      "properties": {
        "audit_entry_id": {
          "$ref": "#/$defs/AuditEntryId"
        },
        "sub": {
          "type": "string"
        }
      },
      "required": [
        "sub",
        "audit_entry_id"
      ],
      "type": "object"
    },
    "CustomerType": {
      "enum": [
        "Individual",
        "GovernmentEntity",
        "PrivateCompany",
        "Bank",
        "FinancialInstitution",
        "ForeignAgencyOrSubsidiary",
        "NonDomiciledCompany"
      ],
      "type": "string"
    },
    "KycLevel": {
      "enum": [
        "NotKyced",
        "Basic",
        "Advanced"
      ],
      "type": "string"
    }
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "oneOf": [
    {
      "properties": {
        "audit_info": {
          "$ref": "#/$defs/AuditInfo"
        },
        "customer_type": {
          "$ref": "#/$defs/CustomerType"
        },
        "email": {
          "type": "string"
        },
        "id": {
          "format": "uuid",
          "type": "string"
        },
        "telegram_id": {
          "type": "string"
        },
        "type": {
          "const": "initialized",
          "type": "string"
        }
      },
      "required": [
        "type",
        "id",
        "email",
        "telegram_id",
        "customer_type",
        "audit_info"
      ],
      "type": "object"
    },
    {
      "properties": {
        "authentication_id": {
          "format": "uuid",
          "type": "string"
        },
        "type": {
          "const": "authentication_id_updated",
          "type": "string"
        }
      },
      "required": [
        "type",
        "authentication_id"
      ],
      "type": "object"
    },
    {
      "properties": {
        "applicant_id": {
          "type": "string"
        },
        "audit_info": {
          "$ref": "#/$defs/AuditInfo"
        },
        "type": {
          "const": "kyc_started",
          "type": "string"
        }
      },
      "required": [
        "type",
        "applicant_id",
        "audit_info"
      ],
      "type": "object"
    },
    {
      "properties": {
        "applicant_id": {
          "type": "string"
        },
        "audit_info": {
          "$ref": "#/$defs/AuditInfo"
        },
        "level": {
          "$ref": "#/$defs/KycLevel"
        },
        "type": {
          "const": "kyc_approved",
          "type": "string"
        }
      },
      "required": [
        "type",
        "applicant_id",
        "level",
        "audit_info"
      ],
      "type": "object"
    },
    {
      "properties": {
        "applicant_id": {
          "type": "string"
        },
        "audit_info": {
          "$ref": "#/$defs/AuditInfo"
        },
        "type": {
          "const": "kyc_declined",
          "type": "string"
        }
      },
      "required": [
        "type",
        "applicant_id",
        "audit_info"
      ],
      "type": "object"
    },
    {
      "properties": {
        "audit_info": {
          "$ref": "#/$defs/AuditInfo"
        },
        "status": {
          "$ref": "#/$defs/AccountStatus"
        },
        "type": {
          "const": "account_status_updated",
          "type": "string"
        }
      },
      "required": [
        "type",
        "status",
        "audit_info"
      ],
      "type": "object"
    },
    {
      "properties": {
        "audit_info": {
          "$ref": "#/$defs/AuditInfo"
        },
        "telegram_id": {
          "type": "string"
        },
        "type": {
          "const": "telegram_id_updated",
          "type": "string"
        }
      },
      "required": [
        "type",
        "telegram_id",
        "audit_info"
      ],
      "type": "object"
    },
    {
      "properties": {
        "audit_info": {
          "$ref": "#/$defs/AuditInfo"
        },
        "email": {
          "type": "string"
        },
        "type": {
          "const": "email_updated",
          "type": "string"
        }
      },
      "required": [
        "type",
        "email",
        "audit_info"
      ],
      "type": "object"
    }
  ],
  "title": "CustomerEvent"
}